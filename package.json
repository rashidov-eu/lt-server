{
  "author": "Roman Shtylman <shtylman@gmail.com>",
  "name": "localtunnel-server",
  "description": "expose localhost to the world",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/localtunnel/server.git"
  },
  "bin": {
    "lt-server": "./bin/server.js",
    "localtunnel-server": "./bin/server.js"
  },
  "scripts": {
    "test": "ts-mocha -n loader=ts-node/esm -p tsconfig.json --check-leaks src/**/*.test.ts",
    "start": "./src/bin/server.ts",
    "build": "rm -rf ./build && tsc --project tsconfig.json",
    "dev": "LOG_LEVEL=debug nodemon -e ts,js,json --watch './src/**/*' --exec './src/bin/server.ts -p 8080 -d localhost'"
  },
  "dependencies": {
    "commander": "13.1.0",
    "dotenv": "16.4.7",
    "human-readable-ids": "1.0.4",
    "jsonwebtoken": "9.0.2",
    "koa": "2.16.1",
    "koa-jwt": "4.0.4",
    "koa-router": "13.0.1",
    "optimist": "0.5.2",
    "pump": "3.0.2",
    "tldjs": "2.3.1",
    "winston": "3.17.0",
    "ws": "8.18.1"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@stylistic/eslint-plugin-ts": "4.2.0",
    "@types/debug": "4.1.12",
    "@types/expect": "1.20.4",
    "@types/jsonwebtoken": "9.0.9",
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "@types/mocha": "10.0.10",
    "@types/node": "~22",
    "@types/optimist": "0.0.33",
    "@types/pump": "1.1.3",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "9.24.0",
    "eslint-plugin-mocha": "10.5.0",
    "globals": "15.14.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "rimraf": "~6.0",
    "supertest": "7.1.0",
    "ts-api-utils": "~2.0",
    "ts-mocha": "11.1.0",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "typescript-eslint": "8.19.1"
  }
}
